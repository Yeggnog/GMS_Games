    width = 3;
}
if(height &lt; 3){
    height = 3;
}
//initialize display lines
while(exploretemp &lt; (height-1)){
    if(height - exploretemp &gt; 2){
        exploretemp += 2;
        displaysize += 1;
    }
}
for(var i=0; i&lt;displaysize; i++){
    display[i] = "";
}
