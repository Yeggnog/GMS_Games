            <string>//advance text by one line
if(buffer == 15 &amp;&amp; keyboard_check_pressed(vk_enter)){
    if(string_length(text) &gt;= (width-2)*(height-2)){
        buffer = 0;
        var temp = string_copy(text,(width-2),string_length(text)-(width-2));
        if(string_length(failText) &gt; 0){
            if(string_length(failText) &gt;= width-2){
                temp += string_copy(failText,0,width-2);
            }else{
                temp += string_copy(failText,0,string_length(failText));
            }
            text = temp;
            //show_debug_message(textboxParse(text, id));
        }
    }
}
//update buffer
if(buffer &lt; 15){
    buffer += 1;
}
</string>
