            <string>//----[Event: create]----

//variables (width and height in units of 8x8 tiles)
width = 38;
height = 10;
depth = 0;
rawText = "COMMAND?";
title = "FIGHTA";
cursorPos = 0;
ancestor = -1;
endparams[0] = -1; //mark as not creating another (0 for MessageBox and 1 for OptionBox)

//fix height if it's odd
if(height%2 != 0){
    height += 1;
}
//start option arrays
for(var i=0; i&lt;((height-2)/2); i++){
    optionText[i] = "SAMPLE";
    optionIndex[i] = 0;
}

//constrain the box
// keep within normal bounds:
// - min width/height for an OptionBox is &lt;4,3&gt;
if(width &lt; 4){
    width = 4;
}
if(height &lt; 3){
    height = 3;
}
// keep it from cutting off text, if possible
var tmp = 0;
for(var i=0; i&lt;array_length_1d(optionText); i++){
    if(string_length(optionText[i]) &gt; tmp){
        tmp = string_length(optionText[i]);
    }
}
tmp += 3;
if(width &lt; tmp){
    width = tmp;
}
if(height &lt; (2*(array_length_1d(optionText)+1))+2){
    height = (2*(array_length_1d(optionText)+1))+2;
}
// keep from going off the screen
if(((x/UI_TILE_SIZE) + width) &gt; SCREEN_WIDTH){
    width = SCREEN_WIDTH - (x/UI_TILE_SIZE);
}
if(((y/UI_TILE_SIZE) + height) &gt; SCREEN_HEIGHT){
    height = SCREEN_HEIGHT - (y/UI_TILE_SIZE);
}
//constrain the title
if(string_length(title) &gt; (width-3)){
    title = string_copy(title, 0, width-3);
}
//*Startup animation goes here*
</string>
          </argument>
