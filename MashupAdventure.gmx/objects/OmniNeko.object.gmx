            <kind>1</kind>
            <string>/// THINGS. I'M BAD AT NAMING THEM.

// a quick tea break
if(!time_on){
    if(tea_break &gt; 0){
        tea_break -= 1;
    }else{
        time_on = true;
    }
}

// update turns
if(after_turn_ping &amp;&amp; time_on){
    after_turn_ping = false;
    time_on = false;
    tea_break = 24;
    if(after_turn_id != -1){
        if(whose_turn == 0){
            //show_debug_message("Reset party_turns["+string(after_turn_id)+"] to true");
            party_turns[after_turn_id] = true;
            after_turn_id = -1;
            var flag = false;
            for(var i=0; i&lt;array_length_1d(party_turns); i++){
                if(!party_turns[i]){
                    flag = true;
                }
            }
            if(!flag){
                // go to the next turn
                whose_turn = 1;
show_debug_message("Enemy Turn");
                for(var i=0; i&lt;array_length_1d(enemy_turns); i++){
                    enemy_turns[i] = false;
                }
                battle_neko.animated = -1;
                accepting_input = false;
            }
        }else{
            //show_debug_message("Reset enemy_turns["+string(after_turn_id)+"] to true");
            enemy_turns[after_turn_id] = true;
            after_turn_id = -1;
            /*var flag = false;
            for(var i=0; i&lt;array_length_1d(enemy_turns); i++){
                if(!enemy_turns[i]){
                    flag = true;
                }
            }*/
            var flag = false;
            if(!flag){
                // go to the next turn
                if(enemy_HP[0] == 0){
                    if(enemy_HP[1] &gt; 0){
                        enemy_HP[0] = enemy_HP[1];
                        enemy_HP[1] = 0;
                    }else{
                        // end battle
show_debug_message("Battle End");
                        room_goto_next();
                    }
                }
                whose_turn = 0;
show_debug_message("Player Turn");
                var count = 0;
                for(var i=0; i&lt;3; i++){
                    if(party_stats[i,0] &gt; 0){
                        party_turns[i] = false;
                    }else{
                        count += 1;
                    }
                }
                if(count == 3){
                    // lose the battle
                    instance_create(0,0,obj_GameOver);
                    
                }else{
                battle_neko.cursor_y = 0;
                battle_neko.animated = -1;
                grid_fall_neko();
                random_row_neko();
                random_row_neko();
                battle_neko.status = 0;
                battle_neko.swaps_left = 4;
                battle_neko.death_counter = 12;
                battle_neko.dying = false;
                accepting_input = true;
                }
            }
        }
        after_turn_id = -1;
    }
}
</string>
