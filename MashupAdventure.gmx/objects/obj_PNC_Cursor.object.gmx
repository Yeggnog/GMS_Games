            <string>/// *clicks furiously on TV*
if(key_A){
    // 'click'
    var ind = 0;
    switch(other.obj){
        case "games": ind = 1; break;
        case "tv":
            if(obj_PNC_Control.times_tv_clicked == 0){
                ind = 2;
            }else if(obj_PNC_Control.times_tv_clicked == 1){
                ind = 9;
            }else if(obj_PNC_Control.times_tv_clicked == 2){
                ind = 10;
            }else if(obj_PNC_Control.times_tv_clicked &lt; 999){
                ind = 11;
            }else{
                ind = 12;
            }
            if(obj_PNC_Control.times_tv_clicked &lt; 999){
                obj_PNC_Control.times_tv_clicked += 1;
            }
        break;
        case "drawer":
            ind = 3;
            obj_PNC_Control.drawer_open = true;
            other.obj = "controller"
        break;
        case "console":
            if(obj_PNC_Control.controller_taken){
                // start the game!
            }else{
                ind = 4;
            }
            break;
        case "stairs":
            if(game_beaten){
                ind = 8;
            }else{
                ind = 5;
            }
        break;
        case "controller":
            ind = 6;
            obj_PNC_Control.controller_taken = true;
            with(other){
                instance_destroy();
            }
        break;
    }
    show_debug_message(obj_PNC_Control.messages[ind]);
}
</string>
