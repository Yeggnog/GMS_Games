            <string>/// controls and gravity handling

// control input
if(key_A){
    // jump
    if(jump == 0){
        jump = 1;
        vsp = -8;
        image_speed = 0;
        // jumping
    }
}
if(key_B &amp;&amp; shots_on_screen &lt; 3){
    // shoot
    // make shot object and set params here
    shoot_decay = 8;
    shots_on_screen += 1;
    var temp_id;
    if(facing == 0){
        temp_id = instance_create(x+16,y,obj_Omni_Bullet);
        temp_id.x_spd = 6;
    }else{
        temp_id = instance_create(x-16,y,obj_Omni_Bullet);
        temp_id.x_spd = -6;
    }
    temp_id.sprite_index = spr_Bullet_RNG;
    temp_id.dmg = 1;
    temp_id.ancestor = id;
    temp_id.system_id = part_system_RNG;
    temp_id.part_id = type_square;
}
if(key_right){
    if(!place_meeting(x+3,y-1,obj_Omni_Solid)){
        // move right
        x += 3;
        facing = 0;
        if(jump == 0){
            // right run sprite
            image_speed = temp / 3;
            if(sprite_index != spr_Lucca_Run_R){
                sprite_index = spr_Lucca_Run_R;
            }
        }
    }
}
if(key_left){
    if(!place_meeting(x-3,y-1,obj_Omni_Solid)){
        // move left
        x -= 3;
        facing = 180;
        if(jump == 0){
            // left run sprite
            image_speed = temp / 3;
            if(sprite_index != spr_Lucca_Run_L){
                sprite_index = spr_Lucca_Run_L;
            }
        }
    }
}

// push out
if(place_meeting(x,y-1,obj_Omni_Solid)){
    y -= 1;
}

// gravity
if(!place_meeting(x,y+vsp,obj_Omni_Solid)){
    vsp += 1;
    if(facing == 0){
        // right
        sprite_index = spr_Lucca_Jump_R;
    }else{
        // left
        sprite_index = spr_Lucca_Jump_L;
    }
    if(vsp &gt;= 0){
        jump = -1;
        // falling
    }
}else if(place_meeting(x,y+1,obj_Omni_Solid)){
    vsp = 0;
    jump = 0;
    // land on ground
    if(!key_left &amp;&amp; !key_right){
        if(facing == 0){
            sprite_index = spr_Lucca_R;
        }else{
            sprite_index = spr_Lucca_L;
        }
    }
}else{
    vsp = floor(vsp/3);
}
y += vsp;

// sprite update
if(shoot_decay &gt; 0){
    shoot_decay -= 1;
    if(sprite_index == spr_Lucca_R){
        sprite_index = spr_Lucca_Shoot_R;
    }
    if(sprite_index == spr_Lucca_L){
        sprite_index = spr_Lucca_Shoot_L;
    }
    if(sprite_index == spr_Lucca_Jump_R){
        sprite_index = spr_Lucca_JumpS_R;
    }
    if(sprite_index == spr_Lucca_Jump_L){
        sprite_index = spr_Lucca_JumpS_L;
    }
    if(sprite_index == spr_Lucca_Run_R){
        saved_img = image_index;
        sprite_index = spr_Lucca_RunS_R;
        image_index = saved_img;
        saved_img = -1;
    }
    if(sprite_index == spr_Lucca_Run_L){
        saved_img = image_index;
        sprite_index = spr_Lucca_RunS_L;
        image_index = saved_img;
        saved_img = -1;
    }
}else{
    shoot_decay = 0;
}
