            <kind>1</kind>
            <string>/// update
if(tea_break &gt; 0){
    tea_break -= 1;
}else{
    if(status == 0){
        // normal
        status = 2;
        tea_break = 20;
    }else if(status == 2){
        // charging
        status = 1;
        shots_to_fire = 5;
        tea_break = 5;
    }else{
        // firing
        if(shots_to_fire &gt; 0){
            // create shot
            var spd = 3;
            var offset = 11;
            if(facing == 180){
                spd = -3;
                offset = -11;
            }
            var temp_id = instance_create(x+offset,y+6,obj_Omni_Bullet);
            temp_id.sprite_index = spr_kiBlast;
            temp_id.x_spd = spd;
            temp_id.dmg = 1;
            temp_id.ancestor = id;
            temp_id.system_id = part_system_RNG;
            temp_id.part_id = type_EnemyShot;
            // updates
            shots_to_fire -= 1;
            tea_break = 5;
        }else{
            status = 0;
            tea_break = 50;
        }
    }
}

if(obj_Lucca.x &gt; x){
    facing = 0;
}else{
    facing = 180;
}

// collision sets tea_break
if(hit_flash &gt; 0){
    hit_flash -= 1;
}
if(place_meeting(x,y,obj_Omni_Bullet)){
    var ind = instance_place(x,y,obj_Omni_Bullet);
    if(ind.ancestor.object_index == obj_Lucca){
        hit_flash = 2;
        tea_break = 20;
        status = 0;
        if(HP &gt; 0){
            HP -= 1;
        }
        ind.destroy_flag = true;
    }
}

// die
if(HP == 0){
    instance_destroy();
}

// update image
if(status == 2 || (status == 1 &amp;&amp; tea_break &lt; 4)){
    // charging
    if(facing == 0){
        image = 2;
    }else{
        image = 7;
    }
}else if(status == 1){
    // firing
    if(facing == 0){
        image = 3;
    }else{
        image = 8;
    }
}else{
    // normal
    if(facing == 0){
        image = 1;
    }else{
        image = 6;
    }
}
image_index = image;
</string>
