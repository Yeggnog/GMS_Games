if(keyboard_check_pressed(vk_enter) &amp;&amp; swaps_left &gt; 0){
    swap_blocks(cursor_x,cursor_y,cursor_x,cursor_y+1);
    swaps_left -= 1;
}

// move the cursor
if(keyboard_check_pressed(vk_left)){
    if(cursor_x &gt; 0){
        cursor_x -= 1;
    }else{
        cursor_x = 5;
    }
}
if(keyboard_check_pressed(vk_right)){
    if(cursor_x &lt; 5){
        cursor_x += 1;
    }else{
        cursor_x = 0;
    }
}
if(keyboard_check_pressed(vk_up)){
    if(cursor_y &gt; 0){
        cursor_y -= 1;
    }else{
        cursor_y = 2;
    }
}
if(keyboard_check_pressed(vk_down)){
    if(cursor_y &lt; 2){
        cursor_y += 1;
    }else{
        cursor_y = 0;
    }
}
