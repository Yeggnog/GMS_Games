if(time_on){

// add a random row
if(keyboard_check_pressed(ord('R'))){
    add_random_row_new();
}

// swap
if(key_A &amp;&amp; swaps_left &gt; 0){
    time_on = false;
    swap_blocks(cursor_x,cursor_y,cursor_x,cursor_y+1);
    swaps_left -= 1;
}

// move the cursor
if(key_left){
    if(cursor_x &gt; 0){
        cursor_x -= 1;
        cursor_offset[0] = 16;
    }else{
        cursor_x = 5;
        cursor_offset[0] = -16*(array_length_2d(block_matrix,0));
    }
}
if(key_right){
    if(cursor_x &lt; 5){
        cursor_x += 1;
        cursor_offset[0] = -16;
    }else{
        cursor_x = 0;
        cursor_offset[0] = 16*(array_length_2d(block_matrix,0));
    }
}
if(key_up){
    if(cursor_y &gt; 0){
        cursor_y -= 1;
        cursor_offset[1] = 16;
    }else{
        cursor_y = 2;
        cursor_offset[1] = -16*(array_height_2d(block_matrix));
    }
}
if(key_down){
    if(cursor_y &lt; 2){
        cursor_y += 1;
        cursor_offset[1] = -16;
    }else{
        cursor_y = 0;
        cursor_offset[1] = 16*(array_height_2d(block_matrix));
    }
}

}
